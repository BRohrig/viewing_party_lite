<h3>Welcome to Viewing Party</h3>
  <div id='users'>
    <div class="sub-header">
    <% if session[:user_id] %>
      <%= link_to "Log Out", logout_path, method: :delete %>
    <% else %>
      <%= button_to 'Create a New User', register_path, method: :get %>
      <%= link_to "Log In", login_path %>
    <% end %>
    </div>
</div>
